<h1>Productos</h1>
<div class="cardsContainer">
    {{#each products}}
    <div class="card">
        <p>{{this.title}}</p>
        <p>{{this.description}}</p>
        <p>{{this.price}}</p>
    </div>
    {{/each}}
</div>

